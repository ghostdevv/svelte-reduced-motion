{
  "name": "svelte-reduced-motion",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostdevv/svelte-reduced-motion.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ghostdevv/svelte-reduced-motion/issues"
  },
  "author": "Willow GHOST <ghostdevbusiness@gmail.com> (https://ghostdev.xyz)",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit sync && svelte-package"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.5",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/package": "^2.3.5",
    "ghostsui": "^1.6.0",
    "mdsvex": "^0.12.3",
    "svelte": "^5.0.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  },
  "peerDependencies": {
    "svelte": "^3.55.0 || ^4.0.0"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./transition": {
      "types": "./dist/transition.d.ts",
      "svelte": "./dist/transition.js",
      "default": "./dist/transition.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "transition": [
        "./dist/transition.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ]
}